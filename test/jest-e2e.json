{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "moduleNameMapper": {
    "^@src/(.*)$": "<rootDir>/../../src/$1",
    "^@test/(.*)$": "<rootDir>../..//test/$1"
  },
  "rootDir": "./apis",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
